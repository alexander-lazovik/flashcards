<!DOCTYPE html>
<html>
  <head>
    <title>Флэшкарточкер</title>
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
  </head>
  <body>
    <div class="container wrapper">
      <%= render 'shared/header' %>
      <div class="row" id="middle">
        <div id="notice_block">
          <%= render 'shared/flash_messages' %>
        </div>
        <div class="col-sm-12">
          <%= yield %>
        </div>
      </div>
    </div>
  </body>
</html>
